# Basics
brew install autoenv bash-completion cowsay gnu-typist mobile-shell mtr nmap s3cmd watch wget

# Git
brew install git git-extras hub

# Vim
brew install ctags macvim

# Search
brew install ack the_silver_searcher

# Mutt
brew install contacts msmtp mutt --sidebar-patch notmuch offline-imap sqlite urlview

# Dev
brew install libmemcached markdown memcached pypy python3 redis solr
